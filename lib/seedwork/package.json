    "@config/tsconfig": "workspace:*",
    "@faker-js/faker": "~8.4.1",
    "@types/degit": "~2.8.6",
    "@types/fs-extra": "~11.0.4",
    "@types/jest": "~29.5.12",
    "@types/lodash": "~4.17.6",
