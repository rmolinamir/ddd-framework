  "version": "1.0.2",
  "private": false,
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "license": "MIT",
  "repository": "rmolinamir/ddd-framework",
  "files": [
    "dist"
  ],
  "types": "dist",
  "main": "./dist/index.js",
  "bin": {
    "ddd-seedwork": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
