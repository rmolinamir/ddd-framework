  },
  "devDependencies": {
    "@config/eslint": "*",
    "@config/tsconfig": "*",
    "@faker-js/faker": "~7.5.0",
    "@types/degit": "~2.8.3",
    "@types/fs-extra": "~9.0.13",
    "@types/jest": "~29.1.2",
    "@types/lodash": "~4.14.186",
    "@types/node": "~18.8.3",
    "jest": "~29.1.2",
    "lodash": "~4.17.21",
    "ts-jest": "~29.0.3",
    "typescript": "~4.8.4"
